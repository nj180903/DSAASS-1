package dsa;

public class Factorial {
    public static int factorial(int num) {
        if (num == 0 || num == 1) {
            return 1;
        } else {
            int factorial = 1;
            for (int i = 2; i <= num; i++) {
                factorial *= i;
            }
            return factorial;
        }
    }
    public static void main(String[] args) {
        int number = 5;
        int result = factorial(number);
        System.out.println("Factorial of " + number " + result);
    }
}
